  @keyframes fadeIn {
    from { opacity: 0; transform: translateY(10px); }
    to { opacity: 1; transform: translateY(0); }
  }
  
  .animate-fadeIn {
    animation: fadeIn 0.6s ease-out forwards;
  }
  
  .animation-delay-200 {
    animation-delay: 0.2s;
  }
  
  .animation-delay-400 {
    animation-delay: 0.4s;
  }
  
  @keyframes cardEntrance {
    from {
      opacity: 0;
      transform: translateY(20px);
    }
    to {
      opacity: 1;
      transform: translateY(0);
    }
  }
  
  .card-entrance {
    animation: cardEntrance 0.4s ease-out forwards;
  }
</style>

<script>
  // Fetch related animals from the same category with improved animation
  document.addEventListener('DOMContentLoaded', function() {
    fetch('/api/animals?category=<%= encodeURIComponent(animal.category) %>&exclude=<%= animal._id %>&limit=4')
      .then(response => response.json())
      .then(data => {
        const container = document.getElementById('related-animals');
        container.innerHTML = ''; // Clear skeleton loaders
        
        if (data.length === 0) {
          container.innerHTML = `
            <div class="col-span-full flex flex-col items-center justify-center text-center p-10">
              <svg xmlns="http://www.w3.org/2000/svg" class="h-16 w-16 text-gray-400 mb-4" fill="none" viewBox="0 0 24 24" stroke="currentColor">
                <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9.172 16.172a4 4 0 015.656 0M9 10h.01M15 10h.01M21 12a9 9 0 11-18 0 9 9 0 0118 0z" />
              </svg>
              <p class="text-lg text-gray-600">No related animals found</p>
            </div>
          `;
          return;
        }
        
        data.forEach((animal, index) => {
          const card = document.createElement('a');
          card.href = `/animal/${animal._id}`;
          card.className = 'block bg-white rounded-xl overflow-hidden shadow-lg hover:shadow-xl transition-all duration-300 transform hover:-translate-y-1 opacity-0';
          card.style.animationDelay = `${index * 0.1}s`;
          
          card.innerHTML = `
            <div class="relative h-48 overflow-hidden">
              <img src="${animal.imageUrl}" alt="${animal.name}" class="w-full h-full object-cover transition-transform duration-700 hover:scale-110">
              <div class="absolute inset-0 bg-gradient-to-t from-black/60 to-transparent opacity-0 hover:opacity-100 transition-opacity duration-300"></div>
            </div>
            <div class="p-4">
              <h3 class="text-lg font-bold text-gray-800 mb-1">${animal.name}</h3>
              <p class="text-sm text-gray-500 italic">${animal.scientificName}</p>
              
              <div class="mt-3 pt-3 border-t border-gray-100 flex justify-between items-center">
                <span class="text-sm text-gray-600">${animal.habitat || 'Unknown habitat'}</span>
                <span class="text-xs px-2 py-1 rounded-full 
                  ${animal.conservationStatus === 'Least Concern' ? 'bg-green-100 text-green-800' : 
                    animal.conservationStatus === 'Near Threatened' ? 'bg-yellow-100 text-yellow-800' : 
                    animal.conservationStatus === 'Vulnerable' ? 'bg-orange-100 text-orange-800' : 
                    animal.conservationStatus === 'Endangered' ? 'bg-red-100 text-red-800' : 
                    'bg-gray-100 text-gray-800'}">
                  ${animal.conservationStatus || 'Unknown'}
                </span>
              </div>
            </div>
          `;
          
          container.appendChild(card);
          
          // Add entrance animation
          setTimeout(() => {
            card.classList.add('card-entrance');
          }, 50);
        });
      })
      .catch(error => {
        console.error('Error fetching related animals:', error);
        document.getElementById('related-animals').innerHTML = `
          <div class="col-span-full flex flex-col items-center justify-center text-center p-10">
            <svg xmlns="http://www.w3.org/2000/svg" class="h-16 w-16 text-red-400 mb-4" fill="none" viewBox="0 0 24 24" stroke="currentColor">
              <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M12 8v4m0 4h.01M21 12a9 9 0 11-18 0 9 9 0 0118 0z" />
            </svg>
            <p class="text-lg text-gray-600">Failed to load related animals</p>
            <button onclick="location.reload()" class="mt-4 px-4 py-2 bg-blue-500 text-white rounded-lg hover:bg-blue-600 transition-colors">
              Try Again
            </button>
          </div>
        `;
      });
  });
</script>

